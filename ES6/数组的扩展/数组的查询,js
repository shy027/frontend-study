let arrs = [
    {name:'aaa', color:'红色'},
    {name:'bbb', color:'绿色'},
    {name:'ccc', color:'红色'},
    {name:'ddd', color:'绿色'},
]

let row1 = arrs.find((row) => row.color === '红色')
console.log(row1);

let row2 = arrs.findLast((row) => row.color === '红色')
console.log(row2);

let row3 = arrs.findLast((row) => row.color === '白色')
console.log(row3);